{
  "name": "tivra-debugmind",
  "displayName": "Tivra DebugMind",
  "description": "AI-powered AWS debugging copilot. Chat with an AI that analyzes CloudWatch logs and fixes your code automatically.",
  "version": "0.2.0-beta",
  "publisher": "tivra",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shrey2525/tivra-debugmind-vscode"
  },
  "bugs": {
    "url": "https://github.com/shrey2525/tivra-debugmind-vscode/issues",
    "email": "info@tivra.ai"
  },
  "homepage": "https://github.com/shrey2525/tivra-debugmind-vscode#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Debuggers",
    "Machine Learning"
  ],
  "keywords": [
    "aws",
    "cloudwatch",
    "debugging",
    "ai",
    "copilot",
    "lambda",
    "logs"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tivra.connectAWS",
        "title": "Tivra: Connect to AWS",
        "icon": "$(cloud)"
      },
      {
        "command": "tivra.startDebugging",
        "title": "Tivra: Start Debugging",
        "icon": "$(bug)"
      },
      {
        "command": "tivra.openCopilot",
        "title": "Tivra: Open DebugMind Copilot",
        "icon": "$(robot)"
      }
    ],
    "configuration": {
      "title": "Tivra DebugMind",
      "properties": {
        "tivra.apiUrl": {
          "type": "string",
          "default": "https://copilot.tivra.ai",
          "description": "Tivra DebugMind API URL"
        },
        "tivra.awsRegion": {
          "type": "string",
          "default": "us-east-1",
          "description": "AWS Region"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --allow-missing-repository"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "@vscode/vsce": "^2.22.0"
  }
}
